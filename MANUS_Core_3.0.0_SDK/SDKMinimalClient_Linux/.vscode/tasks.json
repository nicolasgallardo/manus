{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "--std",
                "c++17",
                "-D",
                "NDEBUG",
                "-g",
                "SDKMinimalClient.cpp",
                "-o",
                "${file}.out",
                "SDKMinimalClient.hpp",
                "ClientPlatformSpecific.cpp",
                "ClientPlatformSpecificTypes.hpp",
                "ClientPlatformSpecific.hpp",
                "-l",
                "ManusSDK",
                "-l",
                "ncurses",
                "-I",
                "./ManusSDK/include",
                "-L",
                "./ManusSDK/lib",
                "-Wl,-rpath",
                "./ManusSDK/lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
